        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NotificationActivity"></activity>
        <activity android:name=".LoadingDrawableActivity" />
        <activity android:name=".LoadingActivity" />
        <activity android:name=".CardViewActivity" />
        <activity android:name=".KotlinMinActivity" />
        <activity android:name=".BannerFlipperActivity" />
        <activity android:name=".BannerPagerActivity" />
        <activity android:name=".MutableStringActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>